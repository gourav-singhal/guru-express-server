    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsome": "^2.3.26",
    "sinon": "^3.0.0",
    "source-map-support": "^0.4.14",
    "tap-difflet": "^0.7.0",
    "tap-min": "^1.2.1",
    "watch": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otissv/guru-express-server.git"
  },
  "author": "Otis Virginie",
